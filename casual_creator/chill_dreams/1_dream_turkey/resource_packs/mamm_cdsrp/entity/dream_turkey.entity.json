{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "mamm_cds:dream_turkey",
      "min_engine_version": "1.12.0",
      "materials": {
        "default": "chicken",
        "legs": "chicken_legs"
      },
      "textures": {
        "default": "textures/mamm/cds/dream_turkey"
      },
      "geometry": {
        "default": "geometry.mamm_cds.dream_turkey"
      },
      "animations": {
        "move": "animation.chicken.move",
        "general": "animation.chicken.general",
        "look_at_target": "animation.common.look_at_target",
        "baby_transform": "animation.chicken.baby_transform"
      },
      "scripts": {
        "animate": [
          "general",
          { "move": "query.modified_move_speed" },
          "look_at_target",
          { "baby_transform": "query.is_baby" }
        ],
        "pre_animation": [
          "variable.wing_flap = ((math.sin(query.wing_flap_position * 57.3) + 1) * query.wing_flap_speed);"
        ]
      },
      "render_controllers": ["controller.render.chicken"],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 0
      }
    }
  }
}
