				},
				"leg3": {
					"position": [0, 1.6, 0],
					"rotation": [0, 0, 0]
				},
				"tail": {
					"rotation": [0, "math.cos(query.life_time * 20.0 * 53.7) * 10.0 -this", 0]
				}
			}
		}
	}
}