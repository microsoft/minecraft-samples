			"loop": true,
			"bones": {
				"body": {
					"position": [0, -1.8, 0],
					"rotation": [0, "math.cos(query.life_time * 20.0 * 53.7) * 5.0 -this", 0]
				},
				"head": {
					"position": [0, -0.8, 0],
					"rotation": ["math.clamp(-5.0 * (query.anim_time / 2.0), -5.0, 0)", 0, "math.clamp(25.0 * (query.anim_time / 2.0), 0, 25.0)"]
